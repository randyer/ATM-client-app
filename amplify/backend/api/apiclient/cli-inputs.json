{
  "version": 1,
  "paths": {
    "/clients": {
      "name": "/clients",
      "lambdaFunction": "clientHandler",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ]
      }
    },
    "/client/{clientId}": {
      "name": "/client/{clientId}",
      "lambdaFunction": "clientHandler",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ]
      }
    }
  }
}